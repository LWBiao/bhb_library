/*bookshop公共侧栏样式*/
#right_aside{
	width:.7rem;
	height:100%;
	position:fixed;
	right:0;
	top:0;
	padding-top:1.8rem;
	margin-right:.12rem;
	/*border:1px solid blue;*/
	}
#right_aside dl{
	/*background:#fff;*/
	/*border:1px solid red;*/
	}
#right_aside dl dt{
	height:.6rem;
	overflow:hidden;
	background-image: url(../images/bs_right_menu.png);
	background-size:.6rem;
	background-repeat: no-repeat;
	background-position: .06rem -.08rem;
	}
#right_aside dl dt.aside_dt01{background-position: .06rem -.88rem;}
#right_aside dl dt.aside_dt02{background-position: .06rem -1.74rem;}
#right_aside dl dt.aside_dt03{background-position: .06rem -2.54rem;}
#right_aside dl dt.aside_dt04{background-position: .06rem -3.4rem;}

#right_aside dl dd{
	font-size:.12rem;
	color:#5e5e5e;
	line-height:.3rem;
	text-align:center;
	margin:0;
	border-bottom:.01rem solid #e7e7e7;
	}



