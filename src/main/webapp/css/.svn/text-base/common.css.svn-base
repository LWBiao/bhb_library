@charset "utf-8";
/*公共部分*/
html {
	font-size: 625%;
}
body,h1,h2,h3,h4,p,ul,ol,form,fieldset,figure {
	margin: 0;
	padding: 0;
}
body {
	background-color: #fefdfe;
	font-family: "微软雅黑","黑体","Helvetica Neue", Helvetica, Arial, "Microsoft Yahei UI", "Microsoft YaHei", SimHei, "\5B8B\4F53", simsun, sans-serif;
	font-size: .16rem;
	height:100%;/*？？为bookshop右侧栏新添加*/
}
ul,ol {
	list-style: outside none none;
}
a,a:hover {
	text-decoration: none;
}
img {
	border:0 solid ;
}
div,figure,figcaption,ul,li{
	box-sizing:border-box;
}
.clear{
	clear:both;
}
.none {
	display: none;
}
/*公共部分结束*/
/*首页背景图*/
body{
	background:url(../images/login_bg.jpg) no-repeat center top;
	}
/*导航*/
#header {
	width: 100%;
	height:.43rem;
	font-size: .18rem;
	padding:.76rem 0 .58rem 0;
}
#header .link{
	width:90%;
	min-width:8rem;
	margin:0 auto;
	}
#header .link a{
	color:#313131;
}
#header .link ul.nav_ul01{
	float:left;
	width:34%;
	margin-left:2%;
}
#header .link ul.nav_ul01 li{
	float:left;
	width:33%;
	line-height:.5rem;
	text-align:center;
}
#header .link .logo{
	width:1.63rem;
	height:.5rem;
	float:left;
	margin-left:5%;
	display:inline-block;
	}
#header .link .logo img{
	width:100%;
	height:100%;
}
#header .link ul.nav_ul02{
	float:left;
	width:34%;
}
#header .link ul.nav_ul02 li{
	float:left;
	width:33%;
	text-align:center;
	line-height:.5rem;
}
#header .link ul.nav_ul03{
	width:15%;
	float:left;
	position:relative;
}
#header .link ul.nav_ul03 li{
	float:left;
	line-height:.5rem;
}
#header .link ul.nav_ul03 li.user em{
	width:.3rem;
	height:.3rem;
	vertical-align:middle;
	margin-right:.1rem;
	background:url(../images/user.jpg) no-repeat center;
	display:inline-block;
	border:1px solid #e7e7e7;
	border-radius:15px;
	}
/*搜索区*/
#header .link ul.nav_ul03 li.search{
	float:right;
	}
.search .bo_search{
	position:absolute;
	width:100%;
	height:.34rem;
	top:.04rem;
	right:.02rem;
	z-index:1000;
	border-radius:.17rem;
	border:.012rem solid #979797;
	padding-left:.2rem;
	font-size:.16rem;
	background:#fff;
	outline:none;
	display:none;
	}
#header .link ul.nav_ul03 li.search img{
	position:absolute;
	top:0;
	right:0;
	z-index:1001;
	cursor:pointer;
	background:inherit;
	width:.45rem;
	height:.45rem;
}
/*导航结束*/
/*主体*/
#container{
	width:98.6%;
	margin:0 auto;
	overflow: hidden;
	margin-top:.58rem;
}
.shadow{
	width:100%;
	height:.6rem;
	position: absolute;
	background:#000;
	bottom:0;
	z-index:1;
	fliter:alpha(opacity=50);
	-moz-opacity:0.5;
	opacity:0.5;
}
#container span{
	width:100%;
	font-size:.16rem;
	color:#fff;
	position: absolute;
	bottom:0;
	left:0;
	z-index:2;
	text-align: center;
	line-height:.6rem;
}
/*左*/
#container .con_left{
	width:48%;
	overflow:hidden;
	float: left;
}
.con_left .left_top{
	width:100%;
	position: relative;
}
.left_top img,.left_bottom_left img,.left_bottom_right img{
	width:100%;
	height:100%;
	border:0 solid;
}
.con_left .left_bottom_left{
	width:49.4%;
	height:3.31rem\9!important;
	height:3.35rem;
	float:left;
	position: relative;
	margin-top:.12rem;
}
.con_left .left_bottom_right{
	width:49.4%;
	height:3.31rem\9!important;
	height:3.35rem;
	float:right;
	margin-top:.12rem;
	position: relative;
}
/*右*/
#container .con_right{
	width:52%;
	overflow:hidden;
	float:right;
}
.con_right .right_left{
	margin-left:1%;
	float: left;
	width:49%;
	height:7.7rem;
	position: relative;
}
.right_left img,.right_top img,.right_bottom img{
	width: 100%;
	height:100%;
}
.con_right .right_right{
	float: right;
	width:49%;
}
.right_right .right_top{
	width:100%;
	height:2.96rem;
	position: relative;
}
.right_right .right_bottom{
	margin-top:.11rem;
	width:100%;
	height:4.63rem;
	position: relative;
}
/*主体结束*/
/*脚部*/
#footer{
	padding:6% 0 1% 0;
}
#footer .foot_content{
	text-align: center;
	margin:0 auto;
	overflow: hidden;
	font-size:.12rem;
 	color:#383838;
}
 .foot_content a{
 	font-size:.12rem;
 	color:#383838;
 }
/*脚部结束*/
